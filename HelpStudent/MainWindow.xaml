<Window x:Class="HelpStudent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelpStudent"
        mc:Ignorable="d" Icon="/Resource/Icons/Logotip.png"
        Title="Регистрация/Вход" Height="1000" Width="540" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height=".05*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="1">
            <Image Source="/Resource/Icons/Logotip.png"  Margin="10"/>
        </Grid>
        <Grid Grid.Row="2">
            <Frame Name="MainScreen" NavigationUIVisibility="Hidden"/>
        </Grid>
    </Grid>
</Window>
